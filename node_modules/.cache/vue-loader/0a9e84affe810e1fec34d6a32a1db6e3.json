.recommends = res.data.recommend.list;\n    })\n    },\n    getHomeGoods(type) {\n      const page = this.goods[type].page + 1\n      getHomeGoods(type,page).then(res => {\n        this.goods[type].list.push(...res.data.list)//es6数组转化\n        this.goods[type].page += 1\n    })\n    }\n  },\n\n}\n</script>\n\n<style>\n  #home {\n    padding-top: 44px;\n  }\n  .home-nav{\n    background-color: var(--color-tint);\n    color: #fff;\n    position: fixed;\n    left: 0;\n    right: 0;\n    top: 0;\n    z-index: 9;\n  }\n  .tab-ctrl {\n    position: sticky;\n    top: 44px;\n  }\n</style>"]}]}